{
  "name": "cineai",
  "version": "2.0.0",
  "description": "Advanced AI-powered movie recommendation engine with voice conversation and hyper-personalized recommendations",
  "keywords": [
    "ai",
    "movie",
    "recommendations",
    "voice-chat",
    "nextjs",
    "typescript",
    "supabase",
    "machine-learning",
    "personalization"
  ],
  "homepage": "https://cineai.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/cineai.git"
  },
  "author": {
    "name": "CineAI Team",
    "email": "contact@cineai.app"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev --turbo",
    "dev:stable": "rm -rf .next node_modules/.cache && next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --watchAll=false",
    "test:coverage:detailed": "jest --coverage --verbose --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:coverage && npm run test:e2e && npm run test:performance",
    "test:performance": "cd performance && npm run test:performance",
    "test:performance:setup": "cd performance && npm install && npx playwright install",
    "test:load": "cd performance && npm run test:load",
    "test:lighthouse": "cd performance && npm run test:lighthouse",
    "test:stress": "cd performance && npm run test:stress",
    "performance:report": "cd performance && npm run generate:report",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:api": "jest src/__tests__/api/ --verbose",
    "test:components": "jest src/__tests__/components/ --verbose",
    "test:integration": "jest src/__tests__/integration/ --verbose",
    "test:lib": "jest src/__tests__/lib/ --verbose",
    "test:hooks": "jest src/__tests__/hooks/ --verbose",
    "test:preferences": "jest src/__tests__/api/movies.test.ts src/__tests__/components/movies-page.test.tsx src/__tests__/components/dashboard.test.tsx src/__tests__/integration/preference-workflow.test.ts --verbose",
    "test:pre-commit": "node src/__tests__/run-tests.js",
    "coverage:open": "open coverage/lcov-report/index.html",
    "coverage:serve": "npx http-server coverage/lcov-report -p 8080",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "npm run security:audit && npm outdated",
    "db:migrate": "npx supabase db push",
    "db:reset": "npx supabase db reset",
    "db:seed": "npx supabase db seed",
    "db:types": "npx supabase gen types typescript --project-id=\"kpgaafgmpnxtnbkwfqdr\" > src/types/supabase.ts",
    "generate:types": "supabase gen types typescript --linked --schema public > src/types/supabase-generated.ts",
    "health:check": "curl -f http://localhost:3000/api/healthz || exit 1",
    "build:analyze": "ANALYZE=true npm run build",
    "deploy:check": "node scripts/deployment-check.js",
    "pre-launch:check": "node scripts/pre-launch-check.js",
    "deploy:staging": "npm run deploy:check && vercel --env=staging",
    "deploy:production": "npm run pre-launch:check && npm run deploy:check && vercel --prod",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.52.0",
    "@hookform/resolvers": "5.0.1",
    "@radix-ui/react-checkbox": "1.3.2",
    "@radix-ui/react-collapsible": "1.1.11",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-slider": "1.3.5",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tabs": "1.1.12",
    "@supabase/auth-helpers-nextjs": "0.10.0",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "2.49.8",
    "@tanstack/react-query": "5.80.7",
    "@types/uuid": "10.0.0",
    "axios": "1.9.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "dotenv": "16.5.0",
    "framer-motion": "12.12.1",
    "lucide-react": "0.511.0",
    "next": "15.3.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.56.4",
    "react-hot-toast": "2.5.2",
    "react-swipeable": "7.0.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "3.3.0",
    "tailwindcss-animate": "1.0.7",
    "use-callback-ref": "^1.3.3",
    "uuid": "11.1.0",
    "zod": "3.25.23"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@playwright/test": "^1.53.1",
    "@tailwindcss/postcss": "4.1.7",
    "@tanstack/react-query-devtools": "5.80.7",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "20.14.2",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "daisyui": "5.0.37",
    "eslint": "9.8.0",
    "eslint-config-next": "15.3.3",
    "eslint-config-prettier": "10.1.5",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "16.0.0",
    "msw": "2.8.4",
    "postcss": "8.4.45",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "4.0.0",
    "typescript": "5.5.4",
    "whatwg-fetch": "3.6.20"
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
